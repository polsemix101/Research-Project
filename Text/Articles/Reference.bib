@Article{Leibovici2019,
  author        = {Leibovici, Didier G. and Claramunt, Christophe},
  journal       = {ENTROPY},
  title         = {On {Integrating} {Size} and {Shape} {Distributions} into a {Spatio}-{Temporal} {Information} {Entropy} {Framework}},
