@Article{Mihailovic2012,
  author        = {Mihailovic, Dragutin T. and Budincevic, Mirko and Perisic, Dusanka and Balaz, Igor},
  journal       = {CHAOS SOLITONS \& FRACTALS},
  title         = {Maps serving the combined coupling for use in environmental models and their behaviour in the presence of dynamical noise},
