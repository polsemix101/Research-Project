  keywords      = {spatio-temporal information, geolocated data, entropy decomposition, permutation entropy, patch size distribution, patch shape distribution, multiple scale, co-occurrences, spatio-temporal data analysis, multiway correspondence analysis, land cover change, LANDSCAPE ECOLOGY},
  language      = {English},
