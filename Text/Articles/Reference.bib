  doi           = {10.3390/e21101001},
  keywords      = {permutation entropy, air temperature fluctuation complexity, spatial variation, temporal variation, driving factors, AIR-TEMPERATURE, CLIMATE-CHANGE, SURFACE-TEMPERATURE, SUMMER MONSOON, TIME-SERIES, VEGETATION, UNCERTAINTIES, FLUCTUATIONS, VARIABILITY, PATTERNS},
