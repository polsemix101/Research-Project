  keywords      = {tropical climate, predictability, entropy, ENSO, IOD, INDIAN-OCEAN DIPOLE, SEA-SURFACE TEMPERATURE, INDONESIAN THROUGHFLOW, EL-NINO, ATMOSPHERIC TELECONNECTIONS, INTERANNUAL VARIABILITY, RECURRENCE PLOTS, ENSO, CMIP5, CIRCULATION},
  language      = {English},
