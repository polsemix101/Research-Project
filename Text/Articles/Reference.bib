  keywords      = {LOGISTIC MAP, POPULATION-DYNAMICS, APPROXIMATE ENTROPY, SAMPLE ENTROPY, TIME-SERIES, CHAOS, BIFURCATION, RANDOMNESS, SYSTEMS},
  language      = {English},
